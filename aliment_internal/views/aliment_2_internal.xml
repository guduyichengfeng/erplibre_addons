<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="aliment_2_internal_view_form" model="ir.ui.view">
        <field name="name">aliment_2_internal_form</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <form string="Titre">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                    <group>
                        <field name="model_1" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="aliment_2_internal_view_graph" model="ir.ui.view">
        <field name="name">aliment_2_internal_graph</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <graph string="Aliment 2 internal">
                <field name="name" type="row" />
                <field name="model_1" type="row" />
            </graph>
        </field>
    </record>

    <record id="aliment_2_internal_view_kanban" model="ir.ui.view">
        <field name="name">aliment_2_internal_kanban</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <kanban class="o_kanban_mobile">
                <field name="name" />
                <field name="model_1" />
                <templates>
                    <t t-name="kanban-box">
                        <div t-attf-class="oe_kanban_global_click">
                            <div class="oe_kanban_details">
                                <ul>
                                    <li class="mb4">
                                        <strong>
                                            <field name="name" />
                                        </strong>
                                    </li>
                                    <li class="mb4">
                                        <strong>
                                            <field name="model_1" />
                                        </strong>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </t>
                </templates>
            </kanban>
        </field>
    </record>

    <record id="aliment_2_internal_view_pivot" model="ir.ui.view">
        <field name="name">aliment_2_internal_pivot</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <pivot string="Aliment 2 internal">
                <field name="name" type="row" />
                <field name="model_1" type="row" />
            </pivot>
        </field>
    </record>

    <record id="aliment_2_internal_view_search" model="ir.ui.view">
        <field name="name">aliment_2_internal_search</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <search string="Aliment 2 internal">
                <filter domain="[('name','!=',False)]" name="name" string="Name" />
                <filter domain="[('model_1','!=',False)]" name="model_1" string="Model 1" />
            </search>
        </field>
    </record>

    <record id="aliment_2_internal_view_tree" model="ir.ui.view">
        <field name="name">aliment_2_internal_tree</field>
        <field name="model">aliment_2.internal</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="model_1" />
            </tree>
        </field>
    </record>

    <act_window
        id="aliment_2_internal_aliment_2_internal_action_window"
        name="Aliment 2 internal"
        res_model="aliment_2.internal"
        view_mode="kanban,tree,pivot,graph,form"
    />
</odoo>
